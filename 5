#include <iostream>
#include <vector>
#include <stack>
using namespace std;

void reverseVector(vector<int>& v, int x) {
    stack<int> st;

    for (int l:v) {
        st.push(l);
    }


    for (int i=0; i<x; i++) {
        v[i]=st.top();
        st.pop();
    }
}

int main() {
    int n;
    cin>>n;

    vector<int> v(n);
    for (int i=0; i<n; i++) {
        cin>>v[i];
    }

    reverseVector(v, n);

    for (int i=0; i<n; i++) {
        cout<<v[i];
        if (i<n-1) cout<<" ";
    }
    cout<<endl;

    return 0;
}
