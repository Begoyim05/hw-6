#include <iostream>
#include <stack>
#include <string>
#include <vector>

using namespace std;

bool Balance(const string& s) {
    stack<char> st;

    for (char c:s) {
        if (c=='(' || c=='[' || c=='{') {
            st.push(c);
        }
        else {
            if (st.empty()) return false;
            char top = st.top();
            st.pop();
            if ((c==')'&&top!='(')||
                (c==']'&&top!='[')||
                (c=='}'&&top!='{')){
                return false;
            }
        }
    }

    return st.empty();
}

int main() {
    string s;
    cin >> s;
    if (Balance(s))
        cout<<"YES"<<endl;
    else
        cout<<"NO"<<endl;
    return 0;
}
